{
  "version": "2.1.0",
  "$schema": "https://json.schemastore.org/sarif-2.1.0.json",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "offLLM prompt regression",
          "informationUri": "https://github.com/ales27pm/offLLM-1",
          "rules": [
            {
              "id": "PROMPT_HASH",
              "name": "Prompt hash regression",
              "shortDescription": {
                "text": "Prompt hash does not match expected hash."
              },
              "helpUri": "https://github.com/ales27pm/offLLM-1",
              "defaultConfiguration": {
                "level": "error"
              }
            },
            {
              "id": "REGISTRY_SCHEMA",
              "name": "Registry schema error",
              "shortDescription": {
                "text": "Prompt registry JSON is invalid or missing required fields."
              },
              "helpUri": "https://github.com/ales27pm/offLLM-1",
              "defaultConfiguration": {
                "level": "error"
              }
            }
          ]
        }
      },
      "results": [
        {
          "ruleId": "REGISTRY_SCHEMA",
          "level": "error",
          "message": {
            "text": "Registry file not found: prompts/registry.json"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "prompts/registry.json"
                }
              }
            }
          ]
        }
      ]
    }
  ]
}
